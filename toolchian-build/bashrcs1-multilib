set +h
umask 022
CLFS=/mnt/clfs
PREFIX_TOOLCHAIN="${CLFS}/toolchain"
LC_ALL=POSIX
PATH=${PREFIX_TOOLCHAIN}/bin:$PATH
export CLFS LC_ALL PATH PREFIX_TOOLCHAIN
unset CFLAGS CXXFLAGS PKG_CONFIG_PATH
export CLFS_ARCH="mips"
export CLFS_HOST="x86_64-cross-linux-gnu"
export CLFS_TARGET="mips64el-linux-gnu"
export BUILD32="-mabi=32"
export BUILDN32="-mabi=n32"
export BUILD64="-mabi=64"
export CLFS_ABI="64"
export MAKEFLAGS="-j8"
export SYSROOT=${CLFS}/sysroot/${CLFS_TARGET}
